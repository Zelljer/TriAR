<Window x:Class="TriAR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TriAR"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <Grid>
        <Image Source="/Resources/TriarLogo.jpg"/>
        <Button Command="{Binding ParseButton_Click}" Content="Парсинг" Style="{StaticResource LeftButton}"/>
        <Button Command="{Binding WeatherButton_Click}" Content="Погода" Style="{StaticResource RightButton}"/>
    </Grid>
</Window>
